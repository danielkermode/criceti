{
  "name": "gosocket",
  "version": "1.0.0",
  "description": "\"A websocket client\"",
  "main": "index.js",
  "engines": {
    "node": "6.2.1"
  },
  "scripts": {
    "test": "mocha --opts ./test/mocha.opts --compilers js:babel-core/register test/tests.js",
    "build": "webpack --progress --profile --colors",
    "build:watch": "webpack --progress --profile --colors --watch",
    "open": "cd ../public && xdg-open index.html",
    "goclean": "cp -r ../vendor/github.com/danielkermode/criceti/chat/ ../",
    "goupdate": "cd .. && govendor update github.com/danielkermode/criceti/chat",
    "gotest": "cd ../vendor/github.com/danielkermode/criceti/chat && go test -v",
    "gobuild": "npm run goclean && git add . && git commit -m 'go build' && git push && npm run goupdate"
  },
  "author": "danielkermode",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "node-jsdom": "^3.1.5",
    "react-addons-test-utils": "^15.2.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  }
}
